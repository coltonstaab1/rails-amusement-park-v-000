<p id="notice">Welcome to the theme park!</p>
<div class="container" role="main">
  <div class="center">
    <div class="pic">
      <img src="http://8900-presscdn-0-84.pagely.netdna-cdn.com/wp-content/themes/beautycon/assets/img/avatar-grey.png" height="250">
    </div>
    <div class="info">
      <p>
        <strong>Name: </strong> <%= @user.name %>
      </p>
      <p>
        <strong>Tickets: </strong> <%= @user.tickets %>
      </p>
      <p>
        <strong>Mood: </strong> <%= @user.mood %>
      </p>
      <p>
        <strong>Height: </strong> <%= @user.height %>
      </p>
      <p>
        <a href="#">Edit</a>
      </p>
      <p>
        <a href="#">See attractions</a>
      </p>
    </div>
  </div>
</div>